{
  "name": "alimhan",
  "version": "1.0.0",
  "private": true,
  "description": "Alimhan E-commerce - Medusa Backend + Next.js Storefront",
  "packageManager": "pnpm@10.11.1",
  "workspaces": [
    "backend",
    "storefront"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:mobile": "./scripts/mobile-dev.sh setup && turbo run dev:mobile",
    "mobile:setup": "./scripts/mobile-dev.sh setup",
    "mobile:restore": "./scripts/mobile-dev.sh restore",
    "mobile:qr": "./scripts/mobile-qr.sh",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "dev:admin": "pnpm --filter @alimhan/admin dev",
    "dev:client": "pnpm --filter @alimhan/client dev",
    "seed": "pnpm --filter @alimhan/admin seed"
  },
  "devDependencies": {
    "turbo": "2.6.3"
  },
  "dependencies": {
    "swiper": "^12.0.3"
  }
}
