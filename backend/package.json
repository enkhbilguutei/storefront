{
  "name": "@alimhan/admin",
  "version": "0.0.1",
  "description": "Alimhan E-commerce - Medusa Backend & Admin",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "node ./scripts/medusa-cli.cjs build",
    "seed": "node ./scripts/medusa-cli.cjs exec ./src/scripts/seed.ts",
    "index-products": "node ./scripts/medusa-cli.cjs exec ./src/scripts/index-products.ts",
    "start": "node ./scripts/medusa-cli.cjs start",
    "dev": "node ./scripts/medusa-cli.cjs develop",
    "dev:mobile": "node ./scripts/medusa-cli.cjs develop -H 0.0.0.0 -p 9000",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.12.0",
    "@medusajs/cache-redis": "^2.12.1",
    "@medusajs/cli": "2.12.0",
    "@medusajs/dashboard": "2.12.0",
    "@medusajs/event-bus-redis": "^2.12.1",
    "@medusajs/framework": "2.12.0",
    "@medusajs/locking-redis": "^2.12.1",
    "@medusajs/medusa": "2.12.0",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "cloudinary": "^2.8.0",
    "jsonwebtoken": "^9.0.3",
    "meilisearch": "^0.54.0",
    "resend": "^6.5.2",
    "scrypt-kdf": "^2.0.1"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.0",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
